  <!-- Script to make header disappear while scrolling down. -->
  <script>
    window.addEventListener("scroll", function () {
      var target = document.getElementsByTagName('h1');
      if (window.pageYOffset > 140) {
        target[0].style.color = "transparent";
        target[0].style.transition = ".2s";
      } else if (window.pageYOffset < 150) {
        target[0].style.color = "white";
        target[0].style.transition = ".2s";
      }
    }, false);
  </script>

  <script type="text/javascript">
    fetch("https://pokeapi.co/api/v2/pokemon/ditto/", {
        "method": "GET",
      })
      .then(res => res.json())
      .then(data => {
        console.log(data);
        document.querySelector('#contentPoke').innerHTML = `<div> ${data.name} </div>`;
      })
      .catch(err => {
        console.log(err);
      });
  </script>

